{
  "name": "async-react",
  "version": "1.0.6",
  "description": "A React component to help with async await",
  "author": "siddharthparmar7",
  "license": "MIT",
  "keywords": [
    "react",
    "react-native",
    "async-await",
    "promises",
    "fetch"
  ],
  "repository": "siddharthparmar7/react-async-await",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
